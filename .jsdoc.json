{
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": [ "jsdoc", "closure" ]
    },
    "source": {
        "include": [ "./app" ],
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "opts": {
        "recurse": true,
        "readme": "README.md"
    },
    "plugins": [ "./jsdoc-ext-plugin.js" ]
}
