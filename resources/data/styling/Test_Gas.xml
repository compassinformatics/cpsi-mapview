<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<StyledLayerDescriptor version="1.1.0" xsi:schemaLocation="http://www.opengis.net/sld StyledLayerDescriptor.xsd" xmlns="http://www.opengis.net/sld" xmlns:ogc="http://www.opengis.net/ogc" xmlns:xlink="http://www.w3.org/1999/xlink" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:se="http://www.opengis.net/se">
    <NamedLayer>
        <se:Name>Gas Stations</se:Name>
        <UserStyle>
            <se:Name>Gas Stations</se:Name>
            <se:Title>Gas Stations</se:Title>
            <se:FeatureTypeStyle>
                <se:Rule>
                    <se:Name>Other</se:Name>
                    <se:PointSymbolizer>
                        <se:Graphic>
                            <se:Mark>
                                <se:WellKnownName>circle</se:WellKnownName>
                                <se:Fill>
                                    <se:SvgParameter name="fill">#00ff00</se:SvgParameter>
                                </se:Fill>
                            </se:Mark>
                            <se:Size>12</se:Size>
                        </se:Graphic>
                    </se:PointSymbolizer>
                </se:Rule>
                <se:Rule>
                    <se:Name>Esso</se:Name>
                    <ogc:Filter>
                        <PropertyIsEqualTo>
                            <PropertyName>name</PropertyName>
                            <Literal>Esso</Literal>
                        </PropertyIsEqualTo>
                    </ogc:Filter>
                    <se:PointSymbolizer>
                        <se:Graphic>
                            <se:Mark>
                                <se:WellKnownName>circle</se:WellKnownName>
                                <se:Fill>
                                    <se:SvgParameter name="fill">#ff0000</se:SvgParameter>
                                </se:Fill>
                            </se:Mark>
                            <se:Size>12</se:Size>
                        </se:Graphic>
                    </se:PointSymbolizer>
                </se:Rule>
                <se:Rule>
                    <se:Name>Texaco</se:Name>
                    <ogc:Filter>
                        <PropertyIsEqualTo>
                            <PropertyName>name</PropertyName>
                            <Literal>Texaco</Literal>
                        </PropertyIsEqualTo>
                    </ogc:Filter>
                    <se:PointSymbolizer>
                        <se:Graphic>
                            <se:Mark>
                                <se:WellKnownName>circle</se:WellKnownName>
                                <se:Fill>
                                    <se:SvgParameter name="fill">#000000</se:SvgParameter>
                                </se:Fill>
                            </se:Mark>
                            <se:Size>12</se:Size>
                        </se:Graphic>
                    </se:PointSymbolizer>
                </se:Rule>
                <se:Rule>
                    <se:Name>Topaz</se:Name>
                    <ogc:Filter>
                        <PropertyIsEqualTo>
                            <PropertyName>name</PropertyName>
                            <Literal>Topaz</Literal>
                        </PropertyIsEqualTo>
                    </ogc:Filter>
                    <se:PointSymbolizer>
                        <se:Graphic>
                            <se:Mark>
                                <se:WellKnownName>circle</se:WellKnownName>
                                <se:Fill>
                                    <se:SvgParameter name="fill">#0000ff</se:SvgParameter>
                                </se:Fill>
                            </se:Mark>
                            <se:Size>12</se:Size>
                        </se:Graphic>
                    </se:PointSymbolizer>
                </se:Rule>
            </se:FeatureTypeStyle>
        </UserStyle>
        <UserStyle>
            <se:Name>Labels</se:Name>
            <se:Title>Gas Station Labels</se:Title>
            <se:FeatureTypeStyle>
                <se:Rule>
                    <se:Name>Labels</se:Name>
                    <se:TextSymbolizer>
                        <se:Label>
                            <ogc:PropertyName>osm_id</ogc:PropertyName>
                        </se:Label>
                        <se:Fill>
                            <se:SvgParameter name="fill">#000000</se:SvgParameter>
                            <se:SvgParameter name="fill-opacity">1</se:SvgParameter>
                        </se:Fill>
                    </se:TextSymbolizer>
                </se:Rule>
            </se:FeatureTypeStyle>
        </UserStyle>
    </NamedLayer>
</StyledLayerDescriptor>
